{
    "name": "techsea/allstack-laravel",
    "description": "Official Laravel SDK for AllStack error tracking and monitoring",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Techsea\\AllStack\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Techsea",
            "email": "info@techsea.sa"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techsea\\AllStack\\AllStackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.1.0"
}