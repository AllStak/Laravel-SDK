{
    "name": "techsea/allstack-laravel",
    "description": "Official Laravel SDK for AllStack error tracking and monitoring",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Techsea\\AllStack\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Techsea",
            "email": "info@techsea.sa"
        }
    ],
    "require": {
        "php": ">=8.0 <9.0",
        "illuminate/support": "^10.0 || ^9.0 || ^8.0 || ^11.0",
        "symfony/http-client": "^7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techsea\\AllStack\\AllStackServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
