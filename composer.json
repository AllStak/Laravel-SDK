{
    "name": "techsea/allstack-laravel",
    "description": "Official Laravel SDK for AllStack error tracking and monitoring",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Techsea\\AllStack\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Techsea",
            "email": "info@techsea.sa"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techsea\\AllStack\\AllStackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}