{
    "name": "techsea/allstack-laravel",
    "description": "Official Laravel SDK for AllStack error tracking and monitoring",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Techsea\\AllStack\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Techsea",
            "email": "info@techsea.sa"
        }
    ],
    "require": {
        "php": ">=7.2 <9.0",
        "illuminate/support": "5.* || 6.* || 7.* || 8.* || 9.* || 10.* || 11.* || 12.*",
        "symfony/http-client": "^4.4 || ^5.4 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.* || 4.* || 5.* || 6.* || 7.* || 8.* || 9.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techsea\\AllStack\\AllStackServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}